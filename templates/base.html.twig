                        <div class="cur-lang-acc-title">
                            {% if app.user %}
                            <h4>BONJOUR: {{ app.user.username }}
                                {% if app.user.filename %}

                                        <img src="{{ vich_uploader_asset(app.user, 'imageFile')|imagine_filter('card') }}" class="img-circle"  alt ="card-img-top">

                                {% else %}
                                    <a class="btn-hover-black" href="{{ path ('produit_show',{ id:item.produit.id, slug:item.produit.slug} ) }} ">
                                        <img src="{{ asset('assets/img/cart/3.jpg') }}" alt="">
                                    </a>
