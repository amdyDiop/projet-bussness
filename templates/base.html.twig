    {#modale du panier #}
    <div class="sidebar-cart onepage-sidebar-area">
        <div class="wrap-sidebar">
            <div class="sidebar-cart-all">
                <div class="sidebar-cart-icon">
                    <button class="op-sidebar-close"><span class="ion-android-close"></span></button>
                </div>
                <div class="cart-content">
                    <h3>Votre Panier </h3>
                    <ul>
                        {% for item in item %}
                            <li class="single-product-cart">
                                <div class="cart-img">
                                    {% if item.produit.filename %}
                                        <a class="btn-hover-black" href="{{ path ('produit_show',{ id:item.produit.id, slug:item.produit.slug} ) }} ">
                                            <img src="{{ vich_uploader_asset(item.produit, 'imageFile')|imagine_filter('card') }}"  alt ="card-img-top"  style="width:100%; height:auto;" >
                                        </a>
                                    {% else %}
                                        <a class="btn-hover-black" href="{{ path ('produit_show',{ id:item.produit.id, slug:item.produit.slug} ) }} ">
                                            <img src="{{ asset('assets/img/cart/3.jpg') }}" alt="">
                                        </a>
                                    {% endif %}

                                </div>
                                <div class="cart-title">
                                    <h3><a href="{{ path ('produit_show',{ id:item.produit.id, slug:item.produit.slug} ) }} "> {{item.produit.labelle}}</a></h3>
                                    <span>{{ item.quantity }} x {{item.produit.prix}}</span>
                                </div>
                                <div class="cart-delete">
                                    <a href="{{path('cart_remove',{'id': item.produit.id})}}"><i class="ion-ios-trash-outline"></i></a>
                                </div>

                            </li>
                            <li class="single-product-cart">
                                <div class="cart-total">
                                    <h4>Total : <span>{{ item.quantity *item.produit.prix}} Cfa</span></h4>
                                </div>
                            </li>
                        {% endfor %}
                        <li class="single-product-cart">
                            <div class="cart-total">
                                <h3> <span><strong>Total: </strong> <span>{{ total}} Cfa</span></span></h3>
                            </div>
                        </li>
                        <li class="single-product-cart">
                            <div class="cart-checkout-btn">
                                <a class="btn-hover cart-btn-style" href="{{path('cart_index')}}">Voir Panier </a>
                                <a class="no-mrg btn-hover cart-btn-style" href="{{ path('finaliser') }}">Finaliser</a>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

